{
  "name": "validation",
  "productName": "Compass Validation package",
  "version": "0.0.1",
  "description": "Compass package to provide a Document Validation rule builder.",
  "main": "index.js",
  "scripts": {
    "ci": "npm run check && npm test",
    "test": "mocha --compilers js:babel-register test/helper.js --recursive ./test/**/*.test.js",
    "fmt": "mongodb-js-fmt ./*.js ./test/*.js",
    "check": "mongodb-js-precommit ./src ./test",
    "compile": "npm run jsx && npm run less",
    "jsx": "babel ./src --out-dir ./lib --presets react,es2015",
    "less": "lessc ./src/styles/index.less ./lib/styles/index.css",
    "prepublish": "npm run compile",
    "pretest": "npm run compile",
    "prestart": "npm run compile",
    "start": "electron ./electron",
    "storybook": "start-storybook -p 9001"
  },
  "precommit": [
    "check"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "hadron-app": "^0.0.1",
    "font-awesome": "^4.6.3",
    "lodash": "^4.15.0",
    "react": "^15.3.0",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.0",
    "react-fontawesome": "^1.1.0",
    "reflux": "^0.4.1",
    "reflux-state-mixin": "^0.7.0",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.2.1",
    "@kadira/storybook": "^2.8.0",
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "debug": "^2.2.0",
    "electron-prebuilt": "^1.2.6",
    "enzyme": "^2.4.1",
    "eslint-config-mongodb-js": "^2.1.0",
    "jsdom": "^9.4.2",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "mongodb-js-fmt": "^0.0.3",
    "mongodb-js-precommit": "^0.2.8",
    "pre-commit": "^1.1.3",
    "sinon": "^1.17.5",
    "watch": "^0.19.2"
  }
}
