{
  "env": {
    "mocha": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "camelcase": 1
  },
  "extends": [
    "mongodb-js/node",
    "mongodb-js/browser"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "plugins": ["react"]
}
