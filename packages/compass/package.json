    "mongodb-data-service": "^22.25.2",
    "mongodb-instance-model": "^12.26.2",
    "mongodb-log-writer": "^2.1.0",
    "mongodb-ns": "^2.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "resolve-mongodb-srv": "^1.1.5",
    "semver": "^7.6.2",
    "sinon": "^8.1.1",
    "source-code-pro": "^2.38.0",
    "ts-node": "^10.9.1",
    "web-vitals": "^2.1.2",
    "winreg-ts": "^1.0.4"
  },
  "optionalDependencies": {
    "macos-export-certificate-and-key": "^1.1.2",
    "win-export-certificate-and-key": "^2.0.1"
  }
}
