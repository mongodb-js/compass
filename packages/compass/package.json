{
  "name": "mongodb-compass",
  "private": true,
  "productName": "MongoDB Compass",
  "description": "The MongoDB GUI",
  "homepage": "https://compass.mongodb.com",
  "version": "0.0.0-dev.0",
  "apiVersion": "3.0.0",
  "main": "build/main.js",
  "author": {
    "name": "MongoDB Inc",
    "email": "compass@mongodb.com"
  },
  "shortcutFolderName": "MongoDB",
  "license": "SSPL",
  "config": {
    "hadron": {
      "endpoint": "https://compass.mongodb.com",
      "protocols": [
        {
          "name": "MongoDB Protocol",
          "schemes": [
            "mongodb"
          ]
        },
        {
          "name": "MongoDB Compass Protocol",
          "schemes": [
            "compass"
          ]
        }
      ],
      "distributions": {
        "default": "compass",
        "compass": {
          "name": "mongodb-compass",
          "productName": "MongoDB Compass",
          "bundleId": "com.mongodb.compass",
          "plugins-directory": ".mongodb/compass/plugins",
          "upgradeCode": "0152273D-2F9F-4913-B67F-0FCD3557FFD1"
        },
        "compass-readonly": {
          "name": "mongodb-compass-readonly",
          "productName": "MongoDB Compass Readonly",
          "bundleId": "com.mongodb.compass.readonly",
          "plugins-directory": ".mongodb/compass-readonly/plugins",
          "upgradeCode": "2176EC1D-EF71-49D4-B3B4-9E15B289D79A",
          "readonly": true
        },
        "compass-isolated": {
          "name": "mongodb-compass-isolated",
          "productName": "MongoDB Compass Isolated Edition",
          "bundleId": "com.mongodb.compass.isolated",
          "plugins-directory": ".mongodb/compass-isolated/plugins",
          "upgradeCode": "516F2BE1-4417-4F31-BAA1-364A59404775",
          "isolated": true
        }
      },
      "build": {
        "win32": {
          "icon": "src/app/images/win32/mongodb-compass.ico",
          "favicon_url": "https://compass.mongodb.com/favicon.ico",
          "loading_gif": "src/app/images/win32/mongodb-compass-installer-loading.gif",
          "background": "src/app/images/win32/background.jpg",
          "banner": "src/app/images/win32/banner.jpg"
        },
        "darwin": {
          "icon": "src/app/images/darwin/mongodb-compass.icns",
          "dmg_background": "src/app/images/darwin/background.png",
          "codesign_identity": "Developer ID Application: MongoDB, Inc. (4XWMY46275)",
          "codesign_sha1": "BBF6BCF14C4365D5632BA5E68B378DA4D8CBDF4C",
          "app_category_type": "public.app-category.productivity"
        },
        "linux": {
          "icon": "src/app/images/linux/mongodb-compass.png",
          "deb_section": "Databases",
          "rpm_categories": [
            "Office",
            "Database",
            "Building",
            "Debugger",
            "IDE",
            "GUIDesigner",
            "Profiling"
          ]
        }
      },
      "asar": {
        "unpack": [
          "**/@mongosh/node-runtime-worker-thread/**",
          "**/interruptor/**",
          "**/kerberos/**",
          "**/snappy/**",
          "**/mongodb-client-encryption/index.js",
          "**/mongodb-client-encryption/package.json",
          "**/mongodb-client-encryption/lib/**",
          "**/mongodb-client-encryption/build/**",
          "**/bl/**",
          "**/nan/**",
          "**/node_modules/bindings/**",
          "**/file-uri-to-path/**",
          "**/bson/**"
        ]
      },
      "rebuild": {
        "onlyModules": [
          "interruptor",
          "keytar",
          "kerberos"
        ]
      }
    }
  },
  "scripts": {
    "install": "node scripts/download-akzidenz.js",
    "electron-rebuild": "electron-rebuild --only kerberos,keytar,interruptor",
    "prestart": "rm -rf package-lock.json && npm run electron-rebuild",
    "start": "npm run webpack serve -- --mode development",
    "pretest": "npm run electron-rebuild",
    "test": "hadron-build test",
    "posttest": "node ../../scripts/killall-mongo.js && node ../../scripts/rebuild.js kerberos keytar interruptor",
    "check": "npm run lint && npm run depcheck",
    "clean": "hadron-build clean",
    "prewebpack": "rm -rf ./build",
    "webpack": "webpack-compass",
    "compile": "npm run webpack -- --mode production",
    "release": "node release/index.js",
    "package-compass": "hadron-build release",
    "test-unit": "npm test -- --unit",
    "test-enzyme": "npm test -- --enzyme",
    "test-packages": "npm test -- --packages",
    "test-main": "npm test -- --main",
    "test-renderer": "npm test -- --renderer",
    "test-functional": "npm test -- --functional",
    "test-release": "npm test -- --release",
    "test-release-tasks": "cross-env MONGODB_DOWNLOADS_AWS_ACCESS_KEY_ID='' mocha --timeout 30000 release/*.spec.js",
    "upload": "hadron-build upload",
    "evergreen-expansions": "hadron-build info --format=yaml --flatten > expansions.yml",
    "jsx": "babel ./src --out-dir ./lib",
    "lint": "eslint \"./src/**/*.{js,jsx}\" \"./test/**/*.js\"",
    "depcheck": "depcheck",
    "pretest-ci": "npm run electron-rebuild",
    "test-ci": "npm run test-release-tasks && xvfb-maybe electron-mocha --no-sandbox ./test/**/*.test.js",
    "posttest-ci": "node ../../scripts/killall-mongo.js && node ../../scripts/rebuild.js kerberos keytar interruptor"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb-js/compass.git"
  },
  "bugs": {
    "url": "https://jira.mongodb.org/projects/COMPASS/issues",
    "email": "compass@mongodb.com"
  },
  "dependencies": {
    "@mongosh/node-runtime-worker-thread": "^1.0.7",
    "kerberos": "^1.1.6",
    "keytar": "^5.1.0"
