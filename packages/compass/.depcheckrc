ignores: [
    '@mongodb-js/prettier-config-compass',
    '@mongodb-js/tsconfig-compass',
    # peer dependency of compass-shell
    '@mongosh/node-runtime-worker-thread',
    # native addons
    'kerberos',
    'keytar',
    'os-dns-native',
    'system-ca',
    'win-export-certificate-and-key',
    'macos-export-certificate-and-key',
