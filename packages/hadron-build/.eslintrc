{
  "env": {
    "mocha": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "extends": ["@mongodb-js/eslint-config-compass/plugin"],
  "plugins": ["mocha"],
  "rules": {
    "mocha/no-mocha-arrows": 0,
    "mocha/no-setup-in-describe": 0,
    "mocha/no-hooks-for-single-case": 0,
    "mocha/no-sibling-hooks": 0,
    "mocha/no-exclusive-tests": 2
  }
}
