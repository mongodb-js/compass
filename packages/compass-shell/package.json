{
  "name": "@mongodb-js/compass-shell",
  "productName": "Compass Shell plugin",
  "version": "0.0.0-dev.0",
  "apiVersion": "3.0.0",
  "description": "Compass Shell Plugin",
  "main": "lib/index.js",
  "scripts": {
    "install": "node scripts/download-akzidenz.js",
    "clean": "rimraf lib",
    "precompile": "npm run clean",
    "compile": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.config.js --progress",
    "compile:watch": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.config.js --progress --watch",
    "prestart": "electron-rebuild -o interruptor",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.dev.config.js",
    "ci": "npm run cover && npm run test:karma && npm run compile && npm run check",
    "prestart:watch": "electron-rebuild -o interruptor",
    "start:watch": "npm run clean && webpack --config ./config/webpack.watch.config.js",
    "start-compass": "node scripts/run-in-compass.js",
    "test": "cross-env NODE_ENV=test mocha-webpack \"./src/**/*.spec.js\"",
    "test-ci": "echo Not tested in CI",
    "test:watch": "cross-env NODE_ENV=test mocha-webpack \"./src/**/*.spec.js\" --watch",
    "test:karma": "npm run prestart && cross-env NODE_ENV=test karma start",
    "cover": "echo",
    "check": "npm run lint && npm run depcheck",
    "link-plugin": "./scripts/link.sh",
    "unlink-plugin": "./scripts/unlink.sh",
    "prepublish": "npm run compile",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "eslint './src/**/*{.js,.jsx}' './test/**/*.js' './electron/**/*.js' './config/**/*{.js,.jsx}'",
    "depcheck": "depcheck --ignores=\"@hot-loader/react-dom,debug,acorn\" || echo \"!!! Dependency check failed, but the failure is ignored by now. This should be addressed in COMPASS-4772 !!!\""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mongodb-js/mongosh.git"
  },
  "config": {
    "unsafe-perm": true
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "precommit": [
    "check"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@leafygreen-ui/icon": "^5.1.0",
    "@leafygreen-ui/icon-button": "^5.0.2",
    "@mongosh/browser-repl": "^0.12.0",
    "@mongosh/node-runtime-worker-thread": "^0.12.0",
    "@mongosh/service-provider-core": "^0.12.0",
    "hadron-react-buttons": "^4.0.4",
