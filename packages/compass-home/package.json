{
  "name": "@mongodb-js/compass-home",
  "productName": "Home plugin",
  "version": "6.2.0",
  "apiVersion": "3.0.0",
  "description": "Home",
  "main": "dist/index.js",
  "compass:main": "src/index.ts",
  "exports": {
    "browser": "./dist/browser.js",
    "require": "./dist/index.js"
  },
  "compass:exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "precompile": "npm run clean",
    "prewebpack": "rimraf ./dist",
    "webpack": "webpack-compass",
    "compile": "npm run webpack -- --mode production",
    "prettier": "prettier",
    "test": "mocha",
    "test-electron": "xvfb-maybe electron-mocha \"./src/**/*.spec.tsx\" --no-sandbox",
    "cover": "nyc npm run test",
    "check": "npm run lint && npm run depcheck",
    "eslint": "eslint",
    "prepublishOnly": "npm run compile && compass-scripts check-exports-exist",
    "lint": "npm run eslint . && npm run prettier -- --check .",
    "depcheck": "compass-scripts check-peer-deps && depcheck",
    "test-cov": "nyc -x \"**/*.spec.*\" --reporter=lcov --reporter=text --reporter=html npm run test",
    "test-ci": "npm run test-cov",
    "test-ci-electron": "npm run test-electron",
    "bootstrap": "npm run compile",
    "reformat": "npm run prettier -- --write ."
  },
  "license": "SSPL",
  "dependencies": {
    "@mongodb-js/compass-components": "^1.1.1",
    "@mongodb-js/compass-settings": "^0.3.0",
    "@mongodb-js/compass-connections": "^1.2.0",
    "hadron-app-registry": "^9.0.1",
    "hadron-ipc": "^3.1.0",
    "mongodb-data-service": "^22.1.1",
    "react": "^16.14.0"
  },
  "peerDependencies": {
    "@mongodb-js/compass-components": "^1.1.1",
    "@mongodb-js/compass-settings": "^0.3.0",
    "@mongodb-js/compass-connections": "^1.2.0",
    "hadron-app-registry": "^9.0.1",
    "hadron-ipc": "^3.1.0",
    "mongodb-data-service": "^22.1.1",
    "react": "^16.14.0"
  },
  "devDependencies": {
    "@mongodb-js/eslint-config-compass": "^1.0.1",
    "@mongodb-js/mocha-config-compass": "^1.0.1",
    "@mongodb-js/prettier-config-compass": "^1.0.0",
    "@mongodb-js/tsconfig-compass": "^1.0.1",
    "@mongodb-js/webpack-config-compass": "^1.0.2",
    "@testing-library/react": "^12.0.0",
    "@types/chai": "^4.2.21",
    "chai": "^4.1.2",
    "debug": "^4.2.0",
    "depcheck": "^1.4.1",
    "electron-mocha": "^10.1.0",
    "eslint": "^7.25.0",
    "eventemitter3": "^4.0.0",
    "mocha": "^8.4.0",
    "mongodb-ns": "^2.4.0",
    "nyc": "^15.0.0",
    "prettier": "^2.7.1",
    "react-dom": "^16.14.0",
    "resolve": "^1.15.1",
    "rimraf": "^3.0.2",
    "sinon": "^8.1.1",
    "xvfb-maybe": "^0.2.1"
  },
  "homepage": "https://github.com/mongodb-js/compass",
  "bugs": {
    "url": "https://jira.mongodb.org/projects/COMPASS/issues",
    "email": "compass@mongodb.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb-js/compass.git"
  }
}
