    "got": "^10.4.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "depcheck": "^1.4.0",
    "electron": "^8.0.1",
    "electron-mocha": "^8.2.0",
    "eslint": "^7.25.0",
    "eslint-config-mongodb-js": "^5.0.3",
    "xvfb-maybe": "^0.2.1"
  },
