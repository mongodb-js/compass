{
  "name": "compass-preferences-model",
  "description": "Compass preferences model",
  "author": "Lucas Hrabovsky <lucas@mongodb.com>",
  "version": "2.43.0",
  "bugs": {
    "url": "https://jira.mongodb.org/projects/COMPASS/issues",
    "email": "compass@mongodb.com"
  },
  "homepage": "https://github.com/mongodb-js/compass",
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb-js/compass.git"
  },
  "files": [
    "dist"
  ],
  "license": "SSPL",
  "main": "dist/index.js",
  "compass:main": "src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/.esm-wrapper.mjs",
      "require": "./dist/index.js"
    },
    "./provider": "./dist/provider.js"
  },
  "compass:exports": {
    ".": "./src/index.ts",
    "./provider": "./src/provider.ts"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "bootstrap": "npm run compile",
    "prepublishOnly": "npm run compile && compass-scripts check-exports-exist",
    "clean": "node -e \"fs.rmSync('dist', { recursive: true, force: true })\" || true",
    "precompile": "npm run clean",
    "compile": "tsc -p tsconfig.json && gen-esm-wrapper . ./dist/.esm-wrapper.mjs",
    "eslint": "eslint-compass",
    "prettier": "prettier-compass",
    "lint": "npm run eslint . && npm run prettier -- --check .",
    "depcheck": "compass-scripts check-peer-deps && depcheck",
    "check": "npm run lint && npm run depcheck",
    "check-ci": "npm run check",
    "test-check-ci": "npm run check && npm test",
    "test": "mocha",
    "test-ci": "npm run test",
    "reformat": "npm run eslint . -- --fix && npm run prettier -- --write ."
  },
  "dependencies": {
    "@mongodb-js/compass-logging": "^1.7.4",
    "@mongodb-js/compass-user-data": "^0.7.4",
    "@mongodb-js/devtools-proxy-support": "^0.4.4",
    "@mongodb-js/compass-components": "^1.41.0",
    "bson": "^6.10.3",
    "@mongodb-js/compass-app-registry": "^9.4.13",
    "hadron-ipc": "^3.5.3",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "@mongodb-js/eslint-config-compass": "^1.4.0",
    "@mongodb-js/mocha-config-compass": "^1.6.8",
    "@mongodb-js/testing-library-compass": "^1.3.3",
    "@mongodb-js/tsconfig-compass": "^1.2.8",
    "@types/js-yaml": "^4.0.5",
    "@types/yargs-parser": "21.0.0",
    "chai": "^4.3.6",
    "depcheck": "^1.4.1",
    "mocha": "^10.2.0",
    "sinon": "^9.2.3"
  }
}
