    "lodash.result": "^4.5.2",
    "lodash.clone": "^4.5.0"
  },
  "peerDependencies": {
    "mongodb-data-service": "^16.0.0"
  },
  "devDependencies": {
    "depcheck": "^1.4.0",
    "electron": "^7.1.10",
    "electron-mocha": "^8.2.0",
    "electron-rebuild": "^1.8.8",
    "eslint": "^7.25.0",
    "eslint-config-mongodb-js": "^5.0.3",
    "mocha": "^7.0.0",
