    "mongodb-ace-autocompleter": "^1.1.1",
    "react": "^16.14.0"
  },
  "devDependencies": {
    "@mongodb-js/compass-export-to-language": "^7.26.1",
    "@mongodb-js/eslint-config-compass": "^0.11.0",
    "@mongodb-js/mocha-config-compass": "^0.12.0",
    "@mongodb-js/prettier-config-compass": "^0.6.0",
