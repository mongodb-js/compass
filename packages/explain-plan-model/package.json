    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "debug": "^2.2.0",
    "depcheck": "^1.4.0",
    "eslint": "^7.25.0",
    "eslint-config-mongodb-js": "^2.2.0",
    "mocha": "^3.1.2",
    "mongodb-js-fmt": "^0.0.3",
    "pre-commit": "^1.1.2"
  },
  "license": "Apache-2.0",
