  "name": "@mongodb-js/compass-find-in-page",
  "productName": "Compass Find In Page plugin",
  "version": "2.0.4",
  "apiVersion": "3.0.0",
  "description": "cmd-f UI for compass",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
