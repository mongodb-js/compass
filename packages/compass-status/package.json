    "eslint-config-mongodb-js": "^2.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "font-awesome": "^4.7.0",
    "hadron-app": "^4.0.0",
    "hadron-app-registry": "^8.0.0",
