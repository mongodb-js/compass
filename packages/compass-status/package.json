{
  "name": "@mongodb-js/compass-status",
  "productName": "Compass Status plugin",
  "version": "4.0.1",
  "apiVersion": "3.0.0",
  "description": "Compass Status Plugin",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "precompile": "npm run clean",
    "compile": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.config.js --progress",
    "start": "webpack-dev-server --config ./config/webpack.dev.config.js",
    "start:watch": "npm run clean && webpack --config ./config/webpack.watch.config.js",
    "test": "cross-env NODE_ENV=test mocha-webpack \"./src/**/*.spec.js\"",
    "test:watch": "cross-env NODE_ENV=test mocha-webpack \"./src/**/*.spec.js\" --watch",
    "cover": "nyc npm run test",
    "ci": "npm run cover && npm run compile && npm run check",
    "fmt": "mongodb-js-fmt ./*.js ./test/*.js",
    "check": "mongodb-js-precommit './src/**/*{.js,.jsx}' './test/**/*.js' './electron/**/*.js' './config/**/*{.js,.jsx}'",
    "link-plugin": "./scripts/link.sh",
    "unlink-plugin": "./scripts/unlink.sh",
    "prepublish": "npm run compile",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "install": "node scripts/download-akzidenz.js"
  },
  "precommit": [
    "check"
  ],
  "license": "Apache-2.0",
  "dependency-check": {
    "ignore": [
      "react-dom"
    ]
  },
  "peerDependencies": {
    "hadron-react-components": "^4.0.4",
    "prop-types": "^15.5.8",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-tooltip": "^3.2.6",
    "reflux": "^0.4.1",
    "reflux-state-mixin": "^0.7.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.0",
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-minify": "^0.2.0",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.9.0",
    "bootstrap": "https://github.com/twbs/bootstrap/archive/v3.3.5.tar.gz",
    "chai": "^4.1.2",
    "chai-enzyme": "1.0.0-beta.0",
    "cheerio": "^1.0.0-rc.2",
    "classnames": "^2.2.5",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.1",
    "electron": "^3.0.7",
    "electron-packager": "^8.7.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint-config-mongodb-js": "^2.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "font-awesome": "^4.7.0",
    "hadron-app": "^3.1.0",
    "hadron-app-registry": "^7.0.0",
    "hadron-react-components": "^4.0.4",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jsdom": "^11.1.0",
    "jsdom-global": "^3.0.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "mocha": "^4.1.0",
    "mocha-webpack": "^1.0.0-rc.1",
    "mongodb-js-fmt": "^0.0.3",
    "mongodb-js-precommit": "^0.3.0",
    "mongodb-reflux-store": "0.0.1",
    "nyc": "^11.0.3",
    "peer-deps-externals-webpack-plugin": "^1.0.2",
    "postcss-loader": "^2.0.6",
    "pre-commit": "^1.1.3",
    "prop-types": "^15.5.8",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-hot-loader": "^4.1.0",
    "react-storybook-addon-chapters": "^1.1.0",
    "reflux": "^0.4.1",
    "reflux-state-mixin": "^0.7.0",
    "resolve": "^1.4.0",
    "rimraf": "^2.6.1",
    "semver": "^5.4.1",
    "sinon": "^3.2.0",
    "sinon-chai": "^2.11.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-node-externals": "^1.6.0",
    "webpack-sources": "1.0.1"
  },
  "dependencies": {
    "lodash.isempty": "^4.4.0"
  }
}
