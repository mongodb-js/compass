    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.16",
    "@storybook/react": "^3.2.0",
    "autoprefixer": "^7.1.2",
    "babel-loader": "^8.2.2",
    "bootstrap": "https://github.com/twbs/bootstrap/archive/v3.3.5.tar.gz",
