    "depcheck": "^1.4.0",
    "electron": "^3.1.13",
    "electron-packager": "^8.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.25.0",
    "eslint-config-mongodb-js": "^2.2.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "font-awesome": "^4.7.0",
    "hadron-app": "^3.2.2",
    "hadron-app-registry": "^7.2.0",
    "hadron-react-components": "^4.0.5",
