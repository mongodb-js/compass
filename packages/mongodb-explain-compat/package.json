{
  "name": "mongodb-explain-compat",
  "version": "3.0.2",
  "description": "Convert mongodb SBE explain output to 4.4 explain output",
  "keywords": [
    "mongodb",
    "sbe",
    "explain"
  ],
  "main": "lib/index.js",
  "compass:main": "lib/index.js",
  "exports": {
    "import": "./.esm-wrapper.mjs",
    "require": "./lib/index.js"
  },
  "compass:exports": {
    ".": "./lib/index.js"
  },
  "files": [
    "LICENSE",
    "lib",
    "package.json",
    "README.md",
    ".esm-wrapper.mjs"
  ],
  "scripts": {
    "lint": "eslint **/*.js",
    "test": "npm run compile && nyc mocha --colors test/*.js",
    "compile": "gen-esm-wrapper . ./.esm-wrapper.mjs",
    "prepublishOnly": "npm run compile && compass-scripts check-exports-exist",
    "test-ci": "npm run test",
    "depcheck": "depcheck",
    "bootstrap": "npm run compile",
    "check": "npm run lint && npm run depcheck",
    "check-ci": "npm run check"
  },
  "homepage": "https://github.com/mongodb-js/compass",
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb-js/compass.git"
  },
  "bugs": {
    "url": "https://jira.mongodb.org/projects/COMPASS/issues",
    "email": "compass@mongodb.com"
  },
  "license": "SSPL",
  "devDependencies": {
    "eslint": "^7.25.0",
    "eslint-config-semistandard": "^15.0.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gen-esm-wrapper": "^1.1.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0"
  }
}
