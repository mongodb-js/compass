{
  "name": "@mongodb-js/compass-crud",
  "productName": "Compass CRUD plugin",
  "version": "9.0.2",
  "description": "Compass Plugin for CRUD Operations",
  "repository": "https://github.com/10gen/compass-crud",
  "main": "lib/index.js",
  "apiVersion": "3.0.0",
  "scripts": {
    "clean": "rimraf lib",
    "precompile": "npm run clean",
    "compile": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.config.js --progress",
    "prestart": "electron-rebuild --force --only keytar",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.dev.config.js",
    "start:watch": "npm run clean && webpack --config ./config/webpack.watch.config.js",
    "pretest": "npm rebuild keytar && mongodb-runner start --port 27018",
    "test": "cross-env NODE_ENV=test mocha-webpack \"./src/**/*.spec.js\"",
    "posttest": "mongodb-runner stop --port 27018",
    "pretest:karma": "electron-rebuild --force --only keytar && mongodb-runner start --port 27018",
    "test:karma": "xvfb-maybe cross-env NODE_ENV=test karma start",
    "posttest:karma": "mongodb-runner stop --port 27018",
    "cover": "nyc npm run test",
    "ci": "npm run check && npm run cover",
    "precheck": "npm run compile",
    "check": "mongodb-js-precommit './src/**/*{.js,.jsx}' './test/**/*.js' './electron/**/*.js' './config/**/*{.js,.jsx}'",
    "prepublishOnly": "npm run compile"
  },
  "dependency-check": {
    "ignore": [
      "@hot-loader/react-dom",
      "@mongodb-js/compass-status",
      "@mongodb-js/compass-query-bar"
    ]
  },
