{
  "name": "@mongodb-js/compass-crud",
  "description": "Compass Plugin for CRUD Operations",
  "author": {
    "name": "MongoDB Inc",
    "email": "compass@mongodb.com"
  },
  "private": true,
  "version": "13.75.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb-js/compass.git"
  },
  "license": "SSPL",
  "homepage": "https://github.com/mongodb-js/compass",
  "bugs": {
    "url": "https://jira.mongodb.org/projects/COMPASS/issues",
    "email": "compass@mongodb.com"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "compass:main": "src/index.ts",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "compass:exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "bootstrap": "npm run compile",
    "compile": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint-compass",
    "prettier": "prettier-compass",
    "lint": "npm run eslint . && npm run prettier -- --check .",
    "depcheck": "compass-scripts check-peer-deps && depcheck",
    "check": "npm run typecheck && npm run lint && npm run depcheck",
    "check-ci": "npm run check",
    "test": "mocha",
    "test-electron": "xvfb-maybe electron-mocha --no-sandbox",
    "test-cov": "nyc --compact=false --produce-source-map=false -x \"**/*.spec.*\" --reporter=lcov --reporter=text --reporter=html npm run test",
    "test-watch": "npm run test -- --watch",
    "test-ci": "npm run test-cov",
    "test-ci-electron": "npm run test-electron",
    "reformat": "npm run eslint . -- --fix && npm run prettier -- --write ."
  },
  "devDependencies": {
    "@mongodb-js/compass-test-server": "^0.3.21",
    "@mongodb-js/eslint-config-compass": "^1.4.10",
    "@mongodb-js/mocha-config-compass": "^1.7.1",
    "@mongodb-js/prettier-config-compass": "^1.2.8",
    "@mongodb-js/testing-library-compass": "^1.3.14",
    "@mongodb-js/tsconfig-compass": "^1.2.10",
    "@types/enzyme": "^3.10.14",
    "@types/reflux": "^6.4.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "depcheck": "^1.4.1",
    "electron": "^37.4.0",
    "electron-mocha": "^12.2.0",
    "enzyme": "^3.11.0",
    "mocha": "^10.2.0",
    "mongodb-instance-model": "^12.46.1",
    "nyc": "^15.1.0",
    "react-dom": "^17.0.2",
    "sinon": "^17.0.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@mongodb-js/compass-app-registry": "^9.4.24",
    "@mongodb-js/compass-app-stores": "^7.61.0",
    "@mongodb-js/compass-assistant": "^1.6.0",
    "@mongodb-js/compass-collection": "^4.74.0",
    "@mongodb-js/compass-components": "^1.52.1",
    "@mongodb-js/compass-connections": "^1.75.0",
    "@mongodb-js/compass-editor": "^0.54.1",
    "@mongodb-js/compass-field-store": "^9.50.0",
    "@mongodb-js/compass-logging": "^1.7.16",
    "@mongodb-js/compass-query-bar": "^8.76.0",
    "@mongodb-js/compass-telemetry": "^1.15.2",
    "@mongodb-js/compass-workspaces": "^0.56.0",
    "@mongodb-js/explain-plan-helper": "^1.4.21",
    "@mongodb-js/my-queries-storage": "^0.41.1",
    "@mongodb-js/reflux-state-mixin": "^1.2.21",
    "@mongodb-js/shell-bson-parser": "^1.2.0",
    "ag-grid-community": "^20.2.0",
    "ag-grid-react": "^20.2.0",
    "bson": "^6.10.4",
    "compass-preferences-model": "^2.54.1",
    "hadron-document": "^8.10.2",
    "hadron-type-checker": "^7.4.21",
    "jsondiffpatch": "^0.5.0",
    "lodash": "^4.17.21",
    "mongodb": "^6.19.0",
    "mongodb-data-service": "^22.33.1",
    "mongodb-ns": "^2.4.2",
    "mongodb-query-parser": "^4.3.0",
    "numeral": "^2.0.6",
    "react": "^17.0.2",
    "reflux": "^0.4.1",
    "semver": "^7.6.3"
  },
  "is_compass_plugin": true
}
