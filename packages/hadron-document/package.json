    "@mongodb-js/tsconfig-compass": "^0.6.0",
    "@types/lodash.isstring": "^4.0.6",
    "@types/lodash.isarray": "^4.0.6",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/lodash.keys": "^4.2.6",
    "@types/lodash.isequal": "^4.5.5",
    "chai": "^4.2.0",
    "depcheck": "^1.4.1",
    "eslint": "^7.25.0",
    "eslint-config-mongodb-js": "^5.0.3",
    "mocha": "^7.0.1",
    "moment": "^2.27.0"
