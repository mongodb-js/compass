  "precommit": [
    "check"
  ],
  "dependencies": {
    "ampersand-model": "^8.0.0",
    "ampersand-rest-collection": "^6.0.0"
  },
  "devDependencies": {
    "depcheck": "^1.4.0",
    "eslint": "^7.25.0",
    "eslint-config-mongodb-js": "^2.0.0",
    "mocha": "^3.0.2",
    "mongodb-js-fmt": "^0.0.3",
    "pre-commit": "^1.1.2"
  }
}
