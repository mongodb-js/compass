{
  "name": "mongodb-connection-fixture",
  "description": "Fixture data used by mongodb-connection-model for testing.",
  "version": "0.0.14",
  "scripts": {
    "check": "npm run lint && npm run depcheck",
    "test": "mocha",
    "ci": "npm run check && npm test",
    "example": "dry=1 mocha examples/mocha.js",
    "lint": "eslint \"./{src,lib,test,bin}/**/*.{js,jsx,ts,tsx}\" \"./*.js\" --no-error-on-unmatched-pattern",
    "depcheck": "depcheck || echo \"!!! Dependency check failed, but the failure is ignored by now. This should be addressed in COMPASS-4772 !!!\""
  },
  "homepage": "http://github.com/mongodb-js/connection-fixture",
  "repository": {
    "type": "git",
    "url": "git://github.com/mongodb-js/connection-fixture.git"
  },
  "dependencies": {
    "debug": "^2.6.0",
    "lodash": "^4.8.1"
  },
  "devDependencies": {
    "depcheck": "^1.4.0",
    "eslint": "^7.25.0",
    "eslint-config-mongodb-js": "^2.0.0",
    "mocha": "^3.0.2",
    "mongodb-connection-model": "^6.3.1",
    "mongodb-js-fmt": "0.0.3",
    "pre-commit": "^1.1.2"
  },
  "license": "Apache-2.0",
  "precommit": [
    "check"
  ]
}
