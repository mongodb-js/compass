{
  "name": "@mongodb-js/compass-field-store",
  "productName": "Compass Compass Field Store package",
  "version": "2.0.0",
  "description": "FieldStore keeps track of available fields in a collection.",
  "main": "index.js",
  "scripts": {
    "ci": "npm run check && npm test",
    "fmt": "mongodb-js-fmt ./*.js ./test/*.js",
    "check": "mongodb-js-precommit './src/**/*{.js,.jsx}' './test/**/*.js' index.js",
    "compile": "babel ./src --out-dir ./lib",
    "prepublish": "npm run compile",
    "pretest": "npm run compile",
    "prestart": "npm run compile",
    "start": "electron ./electron",
    "test": "npm run compile && mocha test"
  },
  "precommit": [
    "check"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.9.0",
    "bootstrap": "https://github.com/twbs/bootstrap/archive/v3.3.5.tar.gz",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.1.0",
    "chai-enzyme": "^0.5.0",
    "debug": "^2.2.0",
    "enzyme": "^2.4.1",
    "eslint-config-mongodb-js": "^2.1.0",
    "font-awesome": "^4.7.0",
    "hadron-app": "^2.0.0",
    "hadron-app-registry": "^7.0.0",
    "hadron-react-components": "^1.1.0",
    "hadron-reflux-store": "0.0.3",
    "hadron-spectron": "^0.0.1",
    "hadron-type-checker": "^2.0.0",
    "jsdom": "^9.4.2",
    "mocha": "^2.5.3",
    "mock-require": "^2.0.2",
    "mongodb-data-service": "^11.0.3",
    "mongodb-js-fmt": "^0.0.3",
    "mongodb-js-precommit": "^0.2.8",
    "pre-commit": "^1.1.3",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-fontawesome": "^1.6.1",
    "reflux": "^0.4.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "mongodb-schema": "^8.0.0",
    "reflux-state-mixin": "^0.7.0"
  }
}
