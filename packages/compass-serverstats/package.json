    "@mongodb-js/tsconfig-compass": "^1.0.3",
    "@types/d3": "^3.5.x",
    "@types/enzyme": "^3.10.14",
    "chai": "^4.1.2",
    "depcheck": "^1.4.1",
    "electron-mocha": "^12.2.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.25.0",
