{
  "name": "@mongodb-js/compass-serverstats",
  "productName": "Compass Server Stats plugin",
  "version": "14.0.1",
  "apiVersion": "3.0.0",
  "description": "Compass Real Time Server Stats Component.",
  "main": "index.js",
  "browser": "browser.js",
  "scripts": {
    "compile": "npm run jsx && npm run less",
    "jsx": "babel ./src --out-dir ./lib --presets react",
    "less": "lessc ./styles/index.less ./styles/index.css",
    "prepublish": "npm run compile",
    "prestart": "npm run compile",
    "storybook": "start-storybook -p 9001",
    "start": "electron ./electron",
    "test": "xvfb-maybe hadron-build test",
    "check": "npm run lint && npm run depcheck",
    "test-unit": "npm test -- --unit",
    "test-enzyme": "npm test -- --enzyme",
    "test-functional": "npm test -- --functional",
    "lint": "eslint \"./src/**/*.{js,jsx}\" \"./test/**/*.js\" index.js",
    "depcheck": "depcheck || echo \"!!! Dependency check failed, but the failure is ignored by now. This should be addressed in COMPASS-4772 !!!\""
  },
  "license": "Apache-2.0",
  "peerDependencies": {
    "hadron-react-buttons": "^3.1.0",
    "hadron-react-components": "^3.2.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "d3": "3.5.17",
    "d3-timer": "1.0.10",
    "lodash.has": "^4.5.2",
    "lodash.isequal": "^4.5.0",
    "lodash.max": "^4.0.1",
    "lodash.remove": "^4.7.0",
    "lodash.round": "^4.0.4",
    "mongodb-js-errors": "^0.5.0",
    "reflux": "0.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-enzyme": "1.0.0-beta.1",
    "cheerio": "^1.0.0-rc.3",
    "debug": "^4.3.1",
    "depcheck": "^1.4.0",
    "electron": "^3.1.13",
    "electron-mocha": "^6.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.25.0",
    "eslint-config-mongodb-js": "^2.2.0",
    "hadron-app": "^3.2.2",
    "hadron-app-registry": "^7.2.0",
    "hadron-build": "github:mongodb-js/hadron-build#evergreen",
    "hadron-react-buttons": "^3.1.0",
    "hadron-react-components": "^3.5.1",
    "hadron-spectron": "^5.0.0",
    "jsdom": "^9.8.3",
    "less": "^3.11.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "mock-require": "^2.0.1",
    "mongodb-data-service": "^16.5.6",
    "mongodb-ns": "^2.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.12.0",
    "reflux-state-mixin": "^0.8.0",
    "sinon": "^1.17.6",
