{
  "name": "@mongodb-js/compass-serverstats",
  "productName": "Compass Server Stats plugin",
  "version": "12.3.0",
  "apiVersion": "2.0.0",
  "description": "Compass Real Time Server Stats Component.",
  "main": "index.js",
  "browser": "browser.js",
  "scripts": {
    "compile": "npm run jsx && npm run less",
    "jsx": "babel ./src --out-dir ./lib --presets react",
    "less": "lessc ./styles/index.less ./styles/index.css",
    "prepublish": "npm run compile",
    "prestart": "npm run compile",
    "storybook": "start-storybook -p 9001",
    "start": "electron ./electron",
    "test": "hadron-build test",
    "check": "mongodb-js-precommit './src/**/*{.js,.jsx}' './test/**/*.js' index.js",
    "fmt": "mongodb-js-fmt ./*.js ./test/*.js",
    "test-unit": "npm test -- --unit",
    "test-enzyme": "npm test -- --enzyme",
    "test-functional": "npm test -- --functional"
  },
  "precommit": [
    "check"
  ],
  "license": "Apache-2.0",
  "peerDependencies": {
    "hadron-react-components": "^3.0.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "dependencies": {
    "d3": "^3.5.17",
    "d3-timer": "^1.0.3",
    "less": "^2.7.1",
    "lodash.has": "^4.5.2",
    "lodash.isequal": "^4.5.0",
    "lodash.max": "^4.0.1",
    "lodash.remove": "^4.7.0",
    "lodash.round": "^4.0.4",
    "mongodb-js-errors": "^0.3.2",
    "reflux": "0.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.9.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.0",
    "chai-enzyme": "1.0.0-beta.0",
    "cheerio": "^1.0.0-rc.2",
    "debug": "mongodb-js/debug#v2.2.3",
    "electron": "^3.0.7",
    "electron-mocha": "^6.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint-config-mongodb-js": "^2.1.0",
    "hadron-app": "^3.1.0",
    "hadron-app-registry": "^7.0.0",
    "hadron-build": "github:mongodb-js/hadron-build#evergreen",
    "hadron-spectron": "^5.0.0",
    "hadron-react-components": "^3.0.0",
    "jsdom": "^9.4.2",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "mock-require": "^2.0.1",
    "mongodb-core": "^3.0.7",
    "mongodb-data-service": "^12.0.0",
    "mongodb-js-fmt": "^0.0.3",
    "mongodb-js-precommit": "^0.2.8",
    "mongodb-ns": "^2.0.0",
    "pre-commit": "^1.1.3",
    "prop-types": "^15.5.8",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "reflux-state-mixin": "^0.7.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "watch": "^0.19.2"
  }
}
