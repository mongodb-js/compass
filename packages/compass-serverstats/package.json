{
  "name": "@mongodb-js/compass-serverstats",
  "productName": "Compass Server Stats plugin",
  "version": "15.13.0",
  "apiVersion": "3.0.0",
  "description": "Compass Real Time Server Stats Component.",
  "main": "lib/index.js",
  "source": "src/index.js",
  "browser": "lib/browser.js",
  "scripts": {
    "compile": "npm run webpack -- --mode production",
    "prewebpack": "rm -rf ./lib",
    "webpack": "webpack-compass",
    "electron-rebuild": "electron-rebuild --only keytar",
    "node-rebuild": "node ../../scripts/rebuild.js keytar",
    "prestart": "npm run electron-rebuild",
    "start": "npm run webpack serve -- --mode development",
    "test:node": "mocha",
    "pretest:electron": "npm run electron-rebuild",
    "test:electron": "xvfb-maybe electron-mocha --no-sandbox",
    "posttest:electron": "npm run node-rebuild",
    "test": "npm run test:node && npm run test:electron",
    "check": "npm run lint && npm run depcheck",
    "lint": "eslint \"./{src,test}/**/*.{js,jsx}\"",
    "depcheck": "depcheck",
    "test-ci": "npm run test",
    "prepublishOnly": "npm run compile",
    "bootstrap": "npm run compile"
  },
  "license": "SSPL",
  "peerDependencies": {
    "d3": "^3.5.17",
    "hadron-react-buttons": "*",
    "hadron-react-components": "*",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "dependencies": {
    "d3": "^3.5.17",
    "d3-timer": "^1.0.3",
    "lodash.has": "^4.5.2",
    "lodash.isequal": "^4.5.0",
    "lodash.max": "^4.0.1",
    "lodash.remove": "^4.7.0",
    "lodash.round": "^4.0.4",
    "mongodb-js-errors": "^0.3.2",
    "reflux": "^0.4.1"
  },
  "devDependencies": {
    "@mongodb-js/mocha-config-compass": "^0.3.0",
    "@mongodb-js/webpack-config-compass": "^0.1.0",
    "chai": "^4.1.2",
    "debug": "^4.1.1",
    "depcheck": "^1.4.1",
    "electron": "^6.1.12",
    "electron-mocha": "^9.3.3",
    "enzyme": "^3.11.0",
    "eslint": "^7.25.0",
    "eslint-config-mongodb-js": "^2.1.0",
    "eslint-plugin-react": "^7.24.0",
    "hadron-app": "^4.13.0",
    "hadron-app-registry": "^8.4.0",
    "hadron-react-buttons": "^5.4.0",
    "hadron-react-components": "^5.6.0",
    "mocha": "^8.4.0",
    "mongodb-connection-model": "^21.8.0",
    "mongodb-data-service": "^21.11.0",
    "mongodb-ns": "^2.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "xvfb-maybe": "^0.2.1"
  },
  "homepage": "https://github.com/mongodb-js/compass",
  "bugs": {
    "url": "https://jira.mongodb.org/projects/COMPASS/issues",
    "email": "compass@mongodb.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb-js/compass.git"
  }
}
