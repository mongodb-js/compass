{
  "name": "@mongodb-js/compass-deployment-awareness",
  "productName": "Compass Deployment Awareness",
  "version": "10.0.6",
  "apiVersion": "3.0.0",
  "description": "Compass Deployment Awareness Plugin",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb-js/compass-deployment-awareness.git"
  },
  "scripts": {
    "clean": "rimraf lib",
    "precompile": "npm run clean",
    "compile": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.config.js --progress",
    "prestart": "electron-rebuild --force --only keytar",
    "start": "webpack-dev-server --config ./config/webpack.dev.config.js",
    "start:watch": "npm run clean && webpack --config ./config/webpack.watch.config.js",
    "test": "cross-env NODE_ENV=test mocha-webpack \"./src/**/*.spec.js\"",
    "test:watch": "cross-env NODE_ENV=test mocha-webpack \"./src/**/*.spec.js\" --watch",
    "test:karma": "xvfb-maybe cross-env NODE_ENV=test karma start",
    "cover": "nyc npm run test",
    "ci": "npm run cover && npm run test:karma && npm run compile && npm run check",
    "precheck": "npm run compile",
    "check": "mongodb-js-precommit './src/**/*{.js,.jsx}' './test/**/*.js' './electron/**/*.js' './config/**/*{.js,.jsx}'",
    "link-plugin": "./scripts/link.sh",
    "unlink-plugin": "./scripts/unlink.sh",
    "prepublishOnly": "npm run compile",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 9001 -c .storybook",
    "storybook:ghpages": "build-storybook -c .storybook -o .ghpages"
  },
  "license": "Apache-2.0",
  "dependency-check": {
    "ignore": [
      "react-dom",
      "debug",
      "hadron-ipc",
      "hadron-react-components",
      "mongodb"
    ]
  },
  "peerDependencies": {
    "hadron-react-buttons": "^4.0.4",
    "hadron-react-components": "^4.0.4",
    "mongodb": "^3.5.3",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
