{
  "name": "@mongodb-js/compass-connect",
  "productName": "Compass Connect Plugin",
  "version": "3.3.2",
  "apiVersion": "2.0.0",
  "description": "Connection Screen Plugin that supports Compass",
  "main": "index.js",
  "scripts": {
    "ci": "npm run check && npm test",
    "fmt": "mongodb-js-fmt ./*.js ./test/*.js",
    "check": "mongodb-js-precommit './src/**/*{.js,.jsx}' './test/**/*.js' index.js",
    "compile": "npm run jsx && npm run less",
    "postcompile": "cp -r styles/fonts lib/styles/",
    "jsx": "babel ./src --out-dir ./lib --presets react",
    "preless": "lessc ./styles/compass/index.less ./lib/styles/compass/index.css",
    "less": "lessc ./styles/index.less ./lib/styles/index.css",
    "link-plugin": "./scripts/link.sh",
    "unlink-plugin": "./scripts/unlink.sh",
    "precompile": "./node_modules/.bin/electron-rebuild",
    "prepublish": "npm run compile",
    "pretest": "npm run compile",
    "prestart": "npm run compile",
    "start": "electron ./electron",
    "test": "hadron-build test",
    "test-enzyme": "npm test -- --enzyme",
    "test-main": "npm test -- --main",
    "test-renderer": "npm test -- --renderer",
    "test-unit": "npm test -- --unit",
    "test-functional": "npm test -- --functional"
  },
  "precommit": [
    "check"
  ],
  "license": "Apache-2.0",
  "peerDependencies": {
    "hadron-ipc": "^1.0.0"
  },
  "devDependencies": {
    "@mongodb-js/compass-auth-kerberos": "^1.0.1",
    "@mongodb-js/compass-auth-ldap": "^1.0.1",
    "@mongodb-js/compass-auth-x509": "^1.0.1",
    "@mongodb-js/compass-status": "^3.0.0",
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.9.0",
    "bootstrap": "https://github.com/twbs/bootstrap/archive/v3.3.5.tar.gz",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.0",
    "chai-enzyme": "1.0.0-beta.0",
    "cheerio": "^1.0.0-rc.2",
    "electron": "^2.0.0",
    "electron-mocha": "^6.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-mongodb-js": "^2.1.0",
    "font-awesome": "^4.7.0",
    "hadron-app": "^3.0.1",
    "hadron-app-registry": "^7.0.0",
    "hadron-build": "^19.0.2",
    "hadron-ipc": "^1.0.0",
    "jsdom": "^9.4.2",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^4.1.0",
    "mongodb-js-fmt": "^0.0.3",
    "mongodb-js-precommit": "^0.2.8",
    "pre-commit": "^1.1.3",
    "prop-types": "^15.5.8",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-fontawesome": "^1.6.1",
    "reflux": "^0.4.1",
    "reflux-state-mixin": "^0.7.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "triejs": "github:rueckstiess/triejs",
    "uuid": "^3.1.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "ampersand-rest-collection": "^5.0.0",
    "hadron-react-buttons": "^2.0.0",
    "hadron-react-components": "^2.0.0",
    "keytar": "mongodb-js/node-keytar#fdef09013f576b7a257ad768939e827882bccef5",
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0",
    "lodash.foreach": "^4.5.0",
    "lodash.isempty": "^4.4.0",
    "lodash.map": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "moment": "^2.18.1",
    "mongodb-connection-model": "^12.3.2",
    "mongodb-data-service": "^12.1.3",
    "mongodb-shell-to-url": "^0.1.0",
    "react-ios-switch": "^0.1.19",
    "react-tooltip": "^3.3.0",
    "storage-mixin": "^0.8.1"
  }
}
