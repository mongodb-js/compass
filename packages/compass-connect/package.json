    "prop-types": "^15.5.8",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-bootstrap": "^0.32.1",
    "react-fontawesome": "^1.6.1",
    "react-ios-switch": "^0.1.19"
  },
