    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-sources": "1.4.3"
  },
  "dependencies": {
    "@leafygreen-ui/button": "^11.0.2",
