    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "test": "npm run lint && jest",
    "test-check-ci": "npm run check && npm test",
    "test-ci": "npm run test"
  },
  "dependencies": {
    "@leafygreen-ui/confirmation-modal": "^2.2.1"
