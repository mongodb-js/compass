{
  "name": "@mongodb-js/compass-components",
  "version": "0.0.1",
  "description": "React Components used in Compass",
  "license": "SSPL",
  "main": "lib/index.js",
  "module": "./lib/.esm-wrapper.mjs",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "check": "npm run lint && npm run depcheck",
    "clean": "rimraf lib",
    "precompile": "npm run clean",
    "compile": "npm run compile-ts && gen-esm-wrapper . ./lib/esm-wrapper.mjs",
    "compile-ts": "tsc -p tsconfig.json",
    "cover": "nyc npm run test",
    "depcheck": "depcheck",
    "lint": "eslint src/**/*.tsx src/**/*.ts",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "test": "npm run lint && jest",
    "test-check-ci": "npm run check && npm test",
    "test-ci": "npm run test"
  },
  "dependencies": {
    "@leafygreen-ui/confirmation-modal": "^2.2.1"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/react": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.25.0",
    "eslint-config-mongodb-js": "^5.0.3",
    "eslint-config-semistandard": "^15.0.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.6.3",
    "nyc": "^15.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb-js/compass.git"
  },
  "homepage": "https://github.com/mongodb-js/compass"
}
