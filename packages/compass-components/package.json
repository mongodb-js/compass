    "@leafygreen-ui/hooks": "^8.1.2",
    "@leafygreen-ui/icon": "^12.0.0",
    "@leafygreen-ui/icon-button": "^15.0.20",
    "@leafygreen-ui/info-sprinkle": "^1.0.3",
