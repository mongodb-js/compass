{
  "env": {
    "mocha": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "expect": true,
    "sinon": true
  },
  "rules": {
    "camelcase": 1,
    "new-cap": 0,
    "no-use-before-define": 0,
    "no-var": 0,
    "prefer-const": 0
  },
  "extends": ["mongodb-js/node", "mongodb-js/browser", "mongodb-js/react"],
  "plugins": ["react"],
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
