{
  "name": "mongodb-compass",
  "description": "The MongoDB GUI.",
  "version": "1.1.2-pre",
  "main": "main.js",
  "electron_version": "0.36.7",
  "product_name": "MongoDB Compass",
  "authors": "MongoDB Inc.",
  "license": "UNLICENSED",
  "private": true,
  "check": {
    "ignore": [
      "build/**",
      "dist/**",
      "src/minicharts/d3-tip.js"
    ]
  },
  "dependency-check": {
    "entries": [
      "./src/index.js",
      "./gulpfile.js",
      "./tasks/darwin.js",
      "./tasks/win32.js",
      "./tasks/linux.js"
    ],
    "ignore": [
      "app",
      "auto-updater",
      "bootstrap",
      "browser-window",
      "clipboard",
      "crash-reporter",
      "dialog",
      "electron",
      "electron-squirrel-startup",
      "font-awesome",
      "glob",
      "ipc",
      "jade",
      "keytar",
      "menu",
      "mongodb-scope-server"
    ]
  },
  "fonts": [
    "node_modules/font-awesome/fonts/fontawesome-webfont*",
    "fonts/*"
  ],
  "browserify": {
    "debug": true,
    "entries": [
      "./src/index.js"
    ]
  },
  "less": {
    "paths": [
      "node_modules/font-awesome/less",
      "node_modules/bootstrap/less"
    ]
  },
  "scripts": {
    "start": "electron .",
    "release": "gulp release",
    "test": "electron-mocha",
    "ci": "npm run check && npm run release && npm test",
    "check": "mongodb-js-precommit --debug ./*.js src/{**/*.js,*.js} test/{**/*.js,*.js} tasks/*.js",
    "fmt": "mongodb-js-fmt ./*.js src/{**/*.js,*.js} test/{**/*.js,*.js} tasks/*.js",
    "clean": "gulp clean:build",
    "dev": "gulp dev"
  },
  "pre-commit": [
    "check"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/10gen/compass.git"
  },
  "dependencies": {
    "ampersand-app": "^1.0.4",
    "ampersand-collection": "^1.5.0",
    "ampersand-collection-filterable": "^0.2.1",
    "ampersand-collection-lodash-mixin": "^2.0.1",
    "ampersand-collection-rest-mixin": "^5.0.0",
    "ampersand-dom-bindings": "^3.7.0",
    "ampersand-filtered-subcollection": "^2.0.4",
    "ampersand-form-view": "^5.1.1",
    "ampersand-input-view": "^5.0.0",
    "ampersand-model": "^6.0.2",
    "ampersand-rest-collection": "^5.0.0",
    "ampersand-router": "^3.0.2",
    "ampersand-select-view": "^4.3.2",
    "ampersand-state": "4.8.2",
    "ampersand-sync-localforage": "^0.1.1",
    "ampersand-view": "^9.0.0",
    "ampersand-view-switcher": "^2.0.0",
    "async": "^1.5.0",
    "backoff": "^2.4.1",
    "bootstrap": "https://github.com/twbs/bootstrap/archive/v3.3.5.tar.gz",
    "d3": "^3.5.6",
    "debug": "^2.2.0",
    "domready": "^1.0.8",
    "electron-compile": "^2.0.7",
    "electron-squirrel-startup": "^0.1.4",
    "event-stream": "^3.3.2",
    "figures": "^1.4.0",
