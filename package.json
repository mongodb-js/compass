{
  "name": "mongodb-compass",
  "productName": "MongoDB Compass",
  "description": "The MongoDB GUI.",
  "version": "1.2.0-dev",
  "main": "src/main.js",
  "authors": "MongoDB Inc.",
  "license": "UNLICENSED",
  "private": true,
  "config": {
    "hadron": {
      "endpoint": "https://compass-mongodb-com.herokuapp.com",
      "protocols": [
        {
          "name": "MongoDB Protocol",
          "schemes": [
            "mongodb"
          ]
        },
        {
          "name": "MongoDB Compass Protocol",
          "schemes": [
            "compass"
          ]
        }
      ],
      "build": {
        "win32": {
          "icon": "src/images/win32/mongodb-compass.ico",
          "favicon_url": "https://raw.githubusercontent.com/mongodb-js/favicon/master/favicon.ico",
          "loading_gif": "src/app/images/win32/mongodb-compass-installer-loading.gif"
        },
        "darwin": {
          "icon": "src/app/images/darwin/mongodb-compass.icns",
          "dmg_background": "src/app/images/darwin/background.png",
          "codesign_identity": "Developer ID Application: Matt Kangas (ZD3CL9MT3L)",
          "codesign_sha1": "90E39AA7832E95369F0FC6DAF823A04DFBD9CF7A",
          "app_bundle_id": "com.mongodb.compass",
          "app_category_type": "public.app-category.productivity"
        }
      }
    }
  },
  "dependency-check": {
    "entries": [
      "./src/main/index.js",
      "./src/app/index.js"
    ],
    "ignore": [
      "electron",
      "marked"
    ]
  },
  "scripts": {
    "start": "hadron-build develop",
    "test": "xvfb-maybe hadron-build test",
    "check": "mongodb-js-precommit ./src/app/*.js ./src/app/**/**/*.js ./src/{app/**/*.js,main/**/*.js} ./test/*.js",
    "ci": "npm run test",
    "clean": "hadron-build clean",
    "compile-ui": "hadron-build ui",
    "pretest": "mongodb-runner start && mongodb-runner stop",
    "fmt": "mongodb-js-fmt ./*.js src/{**/*.js,*.js} test/{**/*.js,*.js}",
    "release": "hadron-build release",
    "test-functional": "npm test -- --functional",
    "test-unit": "npm test -- --unit",
    "test-release": "npm test -- --release",
    "export": "asar e 'dist/MongoDB Compass-darwin-x64/MongoDB Compass.app/Contents/Resources/app.asar' ../app-asar",
    "upload": "hadron-build upload",
    "evergreen-expansions": "hadron-build config --format=yaml --output=expansions.yml --flatten"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/10gen/compass.git"
  },
  "dependencies": {
    "ampersand-app": "^1.0.4",
    "ampersand-collection": "^1.5.0",
    "ampersand-collection-filterable": "^0.2.1",
    "ampersand-collection-lodash-mixin": "^2.0.1",
    "ampersand-dom-bindings": "^3.7.0",
    "ampersand-filtered-subcollection": "^2.0.4",
    "ampersand-form-view": "^5.2.2",
    "ampersand-input-view": "^5.0.0",
    "ampersand-model": "^6.0.2",
    "ampersand-rest-collection": "^5.0.0",
    "ampersand-router": "^3.0.2",
    "ampersand-select-view": "^4.3.2",
    "ampersand-state": "4.8.2",
    "ampersand-view": "^9.0.0",
    "ampersand-view-switcher": "^2.0.0",
    "app-migrations": "^0.1.1",
    "async": "^1.5.0",
    "backoff": "^2.4.1",
    "bootstrap": "https://github.com/twbs/bootstrap/archive/v3.3.5.tar.gz",
    "d3": "^3.5.6",
    "debug": "^2.2.0",
    "debug-menu": "^0.3.0",
    "electron-squirrel-startup": "^0.1.4",
    "font-awesome": "https://github.com/FortAwesome/Font-Awesome/archive/v4.4.0.tar.gz",
    "get-object-path": "azer/get-object-path#74eb42de0cfd02c14ffdd18552f295aba723d394",
    "hadron-auto-update-manager": "^0.0.12",
    "hadron-compile-cache": "^0.0.3",
    "hadron-ipc": "^0.0.7",
    "hadron-module-cache": "^0.0.3",
    "highlight.js": "^8.9.1",
    "jquery": "^2.1.4",
    "kerberos": "mongodb-js/kerberos#bc619b1b9213eb4cdae786cf3fb916fc7be66758",
    "keytar": "mongodb-js/node-keytar#3ac0124c42ab84490c4bb941e03f65c6f010f42a",
    "less": "^2.6.1",
    "less-cache": "^0.23.0",
    "local-links": "^1.4.0",
    "lodash": "^3.10.1",
    "marked": "^0.3.5",
    "marky-mark": "^1.2.1",
    "moment": "^2.10.6",
    "mongodb-collection-model": "^0.2.3",
    "mongodb-connection-model": "^4.0.2",
    "mongodb-data-service": "^0.0.16",
    "mongodb-database-model": "^0.1.2",
    "mongodb-explain-plan-model": "^0.1.3",
    "mongodb-extended-json": "^1.6.0",
    "mongodb-instance-model": "^3.1.2",
    "mongodb-js-metrics": "^1.0.0",
    "mongodb-language-model": "^0.3.3",
    "mongodb-ns": "^1.0.3",
    "mongodb-schema": "^4.2.0",
    "ms": "^0.7.1",
    "npc": "0.0.1",
    "numeral": "^1.5.3",
    "pluralize": "^1.2.1",
    "qs": "^5.2.0",
    "raf": "^3.1.0",
    "rimraf": "^2.5.2",
    "semver": "^5.1.0",
    "storage-mixin": "^0.6.0",
    "turf-destination": "^1.2.1",
    "turf-distance": "^1.1.0",
    "turf-point": "^2.0.1",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "eslint-config-mongodb-js": "^1.0.6",
    "eslint-plugin-react": "^4.1.0",
    "hadron-build": "^0.3.7",
    "mongodb-js-fmt": "^0.0.3",
    "mongodb-js-precommit": "^0.2.8",
    "mongodb-runner": "^3.1.15",
    "spectron": "^0.34.1",
    "xvfb-maybe": "^0.1.3"
  },
  "engines": {
    "node": "^5.0.0",
    "npm": "^3.0.0"
  },
  "bugs": {
    "url": "https://github.com/10gen/compass/issues"
  }
}
