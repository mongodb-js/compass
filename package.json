    "numeral": "^1.5.3",
    "pluralize": "^1.2.1",
    "prop-types": "^15.5.8",
    "qs": "^5.2.0",
    "raf": "^3.1.0",
    "react": "^15.5.4",
    "react-bootstrap": "0.31.0",
    "react-click-outside": "^2.2.0",
    "react-dnd": "^2.2.4",
    "react-dnd-html5-backend": "^2.2.4",
    "react-dom": "^15.5.4",
    "react-fontawesome": "^1.6.1",
