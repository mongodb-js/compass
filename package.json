    "ms": "^0.7.1",
    "ncp": "^2.0.0",
    "node-notifier": "^4.3.1",
    "numeral": "^1.5.3",
    "pluralize": "^1.2.1",
    "qs": "^5.2.0",
    "raf": "^3.1.0",
    "react": "^15.2.1",
    "react-bootstrap": "0.30.2",
    "react-click-outside": "^2.2.0",
    "react-dom": "^15.2.1",
    "react-fontawesome": "1.2.0",
    "react-native-listener": "^1.0.1",
    "react-tooltip": "^2.0.3",
    "reflux": "0.4.1",
    "reflux-state-mixin": "mongodb-js/reflux-state-mixin",
    "semver": "^5.1.0",
