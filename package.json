{
  "name": "mongodb-compass",
  "description": "The MongoDB GUI.",
  "version": "1.1.2-pre",
  "main": "src/main.js",
  "electron_version": "0.36.7",
  "product_name": "MongoDB Compass",
  "authors": "MongoDB Inc.",
  "license": "UNLICENSED",
  "private": true,
  "check": {
    "ignore": [
      "src/minicharts/d3-tip.js"
    ]
  },
  "dependency-check": {
    "entries": [
      "./src/main/index.js",
      "./src/app/index.js",
      "./src/main/index.js",
      "./scripts/postinstall.js",
      "./scripts/postuninstall.js",
      "./scripts/prepublish.js",
      "./scripts/prestart.js"
    ],
    "ignore": [
      "electron",
      "mongodb-scope-server"
    ]
  },
  "scripts": {
    "prestart": "node scripts/prestart.js",
    "start": "node scripts/start.js",
    "//": "Run only the fast unit tests",
    "test": "xvfb-maybe node scripts/test.js",
    "postinstall": "node scripts/postinstall.js",
    "prepublish": "node scripts/prepublish.js",
    "postuninstall": "node scripts/postuninstall.js",
    "check": "mongodb-js-precommit ./src/app/*.js ./src/app/**/**/*.js ./src/{app/**/*.js,main/**/*.js} ./{scripts,test}/*.js",
    "ci": "npm run test",
    "clean": "npm run postuninstall",
    "fmt": "mongodb-js-fmt ./*.js src/{**/*.js,*.js} test/{**/*.js,*.js} scripts/*.js",
    "release": "npm run prepublish",
    "test-functional": "npm test -- --functional",
    "test-unit": "npm test -- --unit",
    "test-release": "npm test -- --release"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/10gen/compass.git"
  },
