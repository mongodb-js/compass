    "react-virtualized": "^9.6.1",
    "reflux": "0.4.1",
    "reflux-state-mixin": "mongodb-js/reflux-state-mixin",
    "semver": "^5.1.0",
    "storage-mixin": "^0.8.1",
    "turf-destination": "^1.2.1",
    "turf-distance": "^1.1.0",
    "turf-point": "^2.0.1",
    "uuid": "^3.0.0",
    "vega-lite": "^1.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.23.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "chai-enzyme": "^0.5.2",
