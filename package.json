    "xor-it": "^1.0.1"
  },
  "devDependencies": {
    "babel-preset-stage-0": "^6.5.0",
    "browserify": "^12.0.1",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "chalk": "^1.1.1",
    "del": "^2.0.2",
    "electron-compilers": "^2.0.6",
    "electron-installer-codesign": "^0.1.1",
    "electron-installer-dmg": "^0.1.0",
    "electron-installer-squirrel-windows": "^1.3.0",
    "electron-license": "^0.1.0",
    "electron-mocha": "^0.8.0",
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "^0.36.8",
    "electron-rebuild": "^1.0.2",
    "eslint": "^1.9.0",
    "eslint-config-mongodb-js": "^1.0.6",
    "glob": "^5.0.15",
    "gulp": "^3.9.0",
    "gulp-jade": "^1.1.0",
    "gulp-less": "^3.0.3",
    "gulp-shell": "^0.5.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.5",
    "gulp-webserver": "^0.9.1",
    "install": "^0.5.0",
    "jadeify": "^4.4.0",
    "mocha": "^2.3.3",
    "mongodb-js-fmt": "^0.0.3",
    "mongodb-js-precommit": "^0.2.8",
    "mongodb-runner": "mongodb-js/runner",
    "npm": "^3.7.3",
    "npm-registry-client": "^7.0.9",
    "pre-commit": "^1.1.2",
    "spectron": "^0.34.1",
    "watchify": "^3.6.0",
    "which": "^1.2.0"
  }
}
