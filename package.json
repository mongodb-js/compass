    "bootstrap": "https://github.com/twbs/bootstrap/archive/v3.3.5.tar.gz",
    "create-react-class": "^15.5.2",
    "d3": "^3.5.6",
    "d3-flextree": "^1.0.3",
    "d3-timer": "^1.0.3",
    "debug": "mongodb-js/debug#v2.2.3",
    "debug-menu": "^0.3.0",
    "detect-coordinates": "^0.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "font-awesome": "^4.7.0",
    "get-object-path": "azer/get-object-path#74eb42de0cfd02c14ffdd18552f295aba723d394",
    "hadron-app": "^0.9.0",
    "hadron-app-registry": "^5.3.0",
    "hadron-auto-update-manager": "^0.0.12",
    "hadron-compile-cache": "^1.0.1",
    "hadron-document": "^2.1.1",
    "hadron-ipc": "^0.0.7",
    "hadron-module-cache": "^0.0.3",
    "hadron-package-manager": "^3.0.1",
    "hadron-react-bson": "^1.2.0",
    "hadron-react-buttons": "^1.0.0",
    "hadron-react-components": "^1.3.0",
    "hadron-reflux-store": "^0.0.2",
    "hadron-style-manager": "^0.1.0",
    "hadron-type-checker": "^2.0.0",
    "highlight.js": "^8.9.1",
    "jquery": "^2.1.4",
    "keytar": "mongodb-js/node-keytar#fdef09013f576b7a257ad768939e827882bccef5",
    "less": "^2.6.1",
    "less-cache": "^0.23.0",
    "local-links": "^1.4.0",
    "lodash": "^3.10.1",
    "marked": "^0.3.5",
