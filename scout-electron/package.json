{
  "name": "scout-electron",
  "description": "Extra functionality for Scout if running in electron.",
  "version": "0.0.0",
  "main": "index.js",
  "homepage": "http://github.com/10gen/scout.git",
  "repository": {
    "type": "git",
    "url": "git://github.com/10gen/scout.git"
  },
  "dependencies": {
    "debug": "^2.1.3"
  },
  "dependency-check": {
    "//": "Ignore electron built-in modules.",
    "ignore": [
      "app",
      "browser-window",
      "crash-reporter",
      "auto-updater",
      "menu"
    ],
    "entries": [
      "index.js",
      "gulpfile.js"
    ]
  },
  "devDependencies": {
    "async": "^0.9.0",
    "gulp": "^3.8.11",
    "keepup": "0.0.7",
    "merge-stream": "^0.1.7",
    "plist": "^1.1.0",
    "which": "^1.0.9"
  },
  "scripts": {
    "start": "gulp",
    "postinstall": "gulp build"
  }
}
